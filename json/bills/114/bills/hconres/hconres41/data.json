  "introduced_at": "2015-04-23", 
  "number": "41", 
  "official_title": "Expressing the sense of Congress that the people of the United States have the Constitutional right to record law enforcement authorities, and they have the full protection of the law to the possession of the recording devices, and full protection of the law regarding data saved on the recording devices.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": null, 
  "sponsor": {
    "bioguide_id": "H001063", 
    "district": "44", 
    "name": "Hahn, Janice", 
