  "introduced_at": "2016-04-29", 
  "number": "5135", 
  "official_title": "To amend the Arms Export Control Act to provide that no regulation issued under section 38(a)(1) of that Act, and no policy or practice in implementing such a regulation, may prohibit the otherwise lawful export for sale or transfer of any firearm silencer, or any component, part, accessory or attachment for any firearm silencer, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Suppressor Export Act", 
  "sponsor": {
    "bioguide_id": "S001192", 
    "district": "2", 
    "name": "Stewart, Chris", 
    "state": "UT", 
    "thomas_id": "02168", 
