  "introduced_at": "2019-01-23", 
  "number": "717", 
  "official_title": "To amend title 18, United States Code, to prohibit a Federal firearms licensee from selling or delivering certain semiautomatic centerfire rifles to a person under 21 years of age, with exceptions for active duty military personnel and full-time law enforcement officers, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Raise the Age Act", 
  "sponsor": {
    "bioguide_id": "B001304", 
    "district": "4", 
    "name": "Brown, Anthony G.", 
    "state": "MD", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2019-01-23", 
  "subjects": [
    "Congressional oversight", 
    "Crime and law enforcement", 
    "Criminal investigation, prosecution, interrogation", 
    "Criminal justice information and records", 
    "Department of Justice", 
