  "introduced_at": "2019-01-03", 
  "number": "125", 
  "official_title": "To provide for grants for States that require sensitivity training for law enforcement officers of that State and to incentivize States to enact laws requiring the independent investigation and prosecution of the use of deadly force by law enforcement officers, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Police Training and Independent Review Act of 2019", 
  "sponsor": {
    "bioguide_id": "C001068", 
    "district": "9", 
    "name": "Cohen, Steve", 
    "state": "TN", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2019-01-03", 
  "subjects": [
    "Crime and law enforcement", 
    "Criminal investigation, prosecution, interrogation", 
    "Disability and health-based discrimination", 
    "Disability and paralysis", 
    "Law enforcement administration and funding", 
