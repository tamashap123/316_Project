    }, 
    {
      "acted_at": "2019-01-30T13:06:42-05:00", 
      "action_code": "H32050", 
      "references": [], 
      "text": "The House resolved into Committee of the Whole House on the state of the Union for further consideration.", 
      "type": "action"
    }, 
    {
      "acted_at": "2019-01-30T13:35:43-05:00", 
      "action_code": "H32600", 
      "links": [
        {
          "name": "H.R. 790", 
          "url": "https://www.congress.gov/bill/116th-congress/house-bill/790"
        }
      ], 
      "references": [], 
      "text": "The House rose from the Committee of the Whole House on the state of the Union to report H.R. 790.", 
      "type": "action"
    }, 
    {
      "acted_at": "2019-01-30T13:36:45-05:00", 
      "action_code": "H35000", 
      "references": [], 
      "text": "The previous question was ordered pursuant to the rule.", 
      "type": "action"
    }, 
    {
      "acted_at": "2019-01-30T13:36:57-05:00", 
      "action_code": "H34400", 
      "links": [
        {
          "name": "H1304", 
          "url": "https://www.congress.gov/congressional-record/volume-165/house-section/page/H1304"
        }
