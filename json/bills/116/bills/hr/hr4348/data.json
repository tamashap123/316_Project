  "short_title": "PAW and FIN Conservation Act of 2019", 
  "sponsor": {
    "bioguide_id": "G000551", 
    "district": "3", 
    "name": "Grijalva, Raul M.", 
    "state": "AZ", 
    "title": "Rep", 
    "type": "person"
