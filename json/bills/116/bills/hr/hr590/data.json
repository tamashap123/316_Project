  "introduced_at": "2019-01-16", 
  "number": "590", 
  "official_title": "To amend the Immigration and Nationality Act to provide that an alien may not be denied admission or entry to the United States, or other immigration benefits, because of the aliens religion, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Freedom of Religion Act of 2019", 
  "sponsor": {
    "bioguide_id": "B001292", 
    "district": "8", 
    "name": "Beyer, Donald S., Jr.", 
    "state": "VA", 
    "title": "Rep", 
    "type": "person"
  }, 
  "status": "REFERRED", 
  "status_at": "2019-01-16", 
  "subjects": [
