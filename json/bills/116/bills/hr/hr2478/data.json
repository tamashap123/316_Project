  "introduced_at": "2019-05-02", 
  "number": "2478", 
  "official_title": "To amend title 5, United States Code, to provide that civilian service in a temporary position after December 31, 1988, may be creditable service under the Federal Employees Retirement System, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Federal Retirement Fairness Act", 
  "sponsor": {
    "bioguide_id": "K000381", 
