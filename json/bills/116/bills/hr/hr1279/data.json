  "introduced_at": "2019-02-14", 
  "number": "1279", 
  "official_title": "To authorize the Director of the Bureau of Justice Assistance to make grants to States, units of local government, and gun dealers to conduct gun buyback programs, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Safer Neighborhoods Gun Buyback Act of 2019", 
  "sponsor": {
    "bioguide_id": "P000604", 
    "district": "10", 
    "name": "Payne, Donald M., Jr.", 
    "state": "NJ", 
    "title": "Rep", 
