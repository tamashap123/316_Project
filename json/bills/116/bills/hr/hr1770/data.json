  "introduced_at": "2019-03-14", 
  "number": "1770", 
  "official_title": "To amend the Internal Revenue Code of 1986 to exclude from gross income certain amounts realized on the disposition of property raised or produced by a student agriculturist, and for other purposes.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Student Agriculture Protection Act of 2019", 
  "sponsor": {
    "bioguide_id": "M001157", 
    "district": "10", 
