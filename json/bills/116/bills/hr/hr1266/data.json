  "introduced_at": "2019-02-14", 
  "number": "1266", 
  "official_title": "To amend title 18, United States Code, to require firearm assembly kits to be considered to be firearms.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Ghost Guns Are Guns Act", 
  "sponsor": {
    "bioguide_id": "E000297", 
