  "introduced_at": "2015-03-19", 
  "number": "1492", 
  "official_title": "To amend the Internal Revenue Code of 1986 to increase and improve the credit for dependent care expenses and to provide a credit for education of employees of child care centers.", 
  "popular_title": null, 
  "related_bills": [], 
  "short_title": "Child Care Access and Refundability Expansion Act", 
  "sponsor": {
    "bioguide_id": "E000290", 
    "district": "4", 
    "name": "Edwards, Donna F.", 
    "state": "MD", 
    "thomas_id": "01894", 
